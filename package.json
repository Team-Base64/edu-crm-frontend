{
  "name": "edu-crm-frontend",
  "private": false,
  "version": "0.0.0",
  "type": "module",
  "browserslist": [
    "> 0.2% and not dead"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "npx eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "npx eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "preview": "vite preview",
    "start": "npm run dev",
    "test": "jest",
    "test:watch": "npm test -- --watchAll",
    "test:update-snapshots": "npm test -- --updateSnapshot",
    "open:coverage": "open ./coverage/index.html"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.2",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.2",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.4",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "browserslist": "^4.22.1",
    "eslint": "^8.45.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-recommended": "^4.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-tsdoc": "^0.2.17",
    "jest": "^29.6.4",
    "jest-environment-jsdom": "^29.6.4",
    "sass": "^1.66.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.0.2",
    "typescript-plugin-css-modules": "^5.0.2",
    "vite": "^4.4.5"
  }
}
