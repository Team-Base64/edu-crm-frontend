{
  "name": "edu-crm-frontend",
  "private": false,
  "browserslist": [
    "> 0.2% and not dead"
  ],
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "start:mock": "export NODE_ENV=mock && vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "preview": "vite preview",
    "getcssvars": "sass ./src/ui-kit/devGetVariables.scss /tmp/app/c.css",
    "start:socketMock": " npx ts-node --esm __mocks__/webSocket.ts ",
    "init:msw": "npx msw init __mocks__/"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.2"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/ws": "^8.5.8",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "browserslist": "^4.22.1",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-recommended": "^4.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-tsdoc": "^0.2.17",
    "mock-socket": "^9.3.1",
    "msw": "^2.0.0",
    "prettier": "3.0.3",
    "sass": "^1.69.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2",
    "typescript-plugin-css-modules": "^5.0.2",
    "unplugin-fonts": "^1.0.3",
    "vite": "^4.4.5",
    "vite-plugin-svgr": "^4.1.0",
    "vite-tsconfig-paths": "^4.2.1",
    "ws": "^8.14.2"
  },
  "msw": {
    "workerDirectory": ""
  }
